{
    "metadata": {
      "version": "2.1.0",
      "lastUpdated": "2025-12-17",
      "author": "Coach Lens Design Team",
      "description": "Modern Athletic Minimalism design system for Coach Lens - AI-powered coaching platform for middle school athletes",
      "targetPlatform": "Desktop Web Application (Mobile-responsive)",
      "componentLibrary": "Material-UI (MUI) v5+ with custom theme",
      "designStyle": "Modern Athletic Minimalism",
      "targetAudience": [
        "Youth sports coaches (primary)",
        "Middle school athletes (ages 10-14)",
        "Parents (secondary, view-only)"
      ]
    },
  
    "brand": {
      "name": "Coach Lens",
      "tagline": "See Progress, Build Champions",
      "personality": [
        "Clean & Professional",
        "Energetic & Athletic",
        "Trustworthy & Data-Driven",
        "Approachable & Encouraging"
      ],
      "voiceTone": {
        "description": "Professional but not clinical, encouraging but not patronizing, data-driven but not overwhelming. Modern and energetic while maintaining coach credibility.",
        "examples": {
          "good": [
            "Great progress this week!",
            "Let's work on improving balance",
            "Your athletes are showing consistent improvement",
            "Ready to start the next assessment?"
          ],
          "avoid": [
            "Patient has improved",
            "Subject demonstrates",
            "Clinical assessment indicates",
            "ERROR: Test failed"
          ]
        }
      },
      "competitorDifferentiation": {
        "kemtai": "Bright green (clinical/medical) - we avoid this, using athletic blue instead",
        "onform": "Black logo (very sporty but stark) - we're warmer and more approachable with generous white space"
      },
      "logo": {
        "type": "Wordmark with optional icon",
        "style": "Clean, modern sans-serif (Jost)",
        "variations": {
          "primary": "Full color on light backgrounds",
          "reversed": "White on dark backgrounds",
          "monochrome": "Single color for small sizes or limited-color applications"
        },
        "minSize": "120px width for digital",
        "clearSpace": "Minimum 24px on all sides"
      },
      "designPhilosophy": "Professional coaching tool that energizes athletes. Trust through data, excitement through movement. Every interaction should feel confident and capable.",
      "targetFeeling": "Like a great assistant coach - helpful, energetic, trustworthy, organized"
    },
  
    "tokens": {
      "colors": {
        "reasoning": "Athletic Blue for trust/professionalism, Energy Orange for energy/action. Clean minimalist palette avoids medical green (Kemtai) and stark black (Onform).",
        
        "brand": {
          "primary": {
            "value": "#2563EB",
            "name": "Athletic Blue",
            "light": "#60A5FA",
            "dark": "#1E40AF",
            "contrastText": "#FFFFFF",
            "usage": ["Primary CTAs", "Key data highlights", "Active states", "Progress indicators", "Navigation"],
            "accessibility": "4.5:1 contrast on white, AAA compliant",
            "muiMapping": "primary.main"
          },
          "secondary": {
            "value": "#60A5FA",
            "name": "Lighter Blue",
            "light": "#93C5FD",
            "dark": "#3B82F6",
            "contrastText": "#FFFFFF",
            "usage": ["Complements primary", "Secondary CTAs", "Less prominent actions"],
            "accessibility": "4.5:1 contrast on white",
            "note": "MUI theme uses lighter blue for secondary to avoid orange overuse. Energy Orange (#F97316) is used via warning color.",
            "muiMapping": "secondary.main (#60A5FA)",
            "energyOrange": {
              "value": "#F97316",
              "muiMapping": "warning.main",
              "usage": "Use warning color for Energy Orange accents"
            }
          }
        },
  
        "neutral": {
          "charcoal": {
            "value": "#2D2D2D",
