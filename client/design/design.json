        "iconColor": "#BDBDBD",
        "textSize": "16px",
        "headingSize": "24px",
        "actionButton": "contained primary",
        "spacing": "24px gap between elements",
        "examples": {
          "noAthletes": {
            "icon": "PeopleOutline",
            "iconLibrary": "@mui/icons-material",
            "heading": "No athletes yet",
