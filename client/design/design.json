          "muiMapping": {
            "default": "background.default (#F5F5F5)",
            "paper": "background.paper (#FFFFFF)"
          },
          "note": "Updated v2.1.0: Default background is light gray to create contrast with white cards"
        },
  
        "muiGreyScale": {
          "50": "#FAFAFA",
          "100": "#F5F5F5",
          "200": "#EEEEEE",
          "300": "#E0E0E0",
          "400": "#BDBDBD",
          "500": "#9E9E9E",
          "600": "#757575",
          "700": "#616161",
          "800": "#424242",
          "900": "#212121"
        },
  
        "skeletonOverlay": {
          "reasoning": "Traffic light system is universally understood by coaches and athletes for real-time movement feedback",
          "states": {
            "good": {
              "color": "#10B981",
              "name": "Success Green",
              "usage": "Proper form, good positioning, test going well",
              "muiChipColor": "success"
            },
            "warning": {
              "color": "#F59E0B",
              "name": "Warning Orange",
              "usage": "Form deviation, approaching failure, needs correction",
              "muiChipColor": "warning"
            },
            "fail": {
              "color": "#EF4444",
              "name": "Error Red",
              "usage": "Failure detected, hands off hips, foot touchdown, test ended",
              "muiChipColor": "error"
            },
            "neutral": {
              "color": "#60A5FA",
              "name": "Neutral Blue",
              "usage": "Setup phase, no assessment yet, waiting to start",
              "muiChipColor": "info"
            }
          },
          "style": {
            "lineWidth": "3px",
            "jointMarkerSize": "8px",
            "jointMarkerStyle": "circle with dot center",
            "opacity": "0.85",
            "opacityExplanation": "Semi-transparent so coaches can see athlete's actual body beneath the skeleton. 1.0 = fully opaque (blocks view), 0.0 = invisible. 0.85 is optimal - visible but not obstructive."
          }
        },
  
        "scoreColorSystem": {
          "reasoning": "5-point scale familiar to coaches (like grades). Color progression from red to green is intuitive.",
          "scale": {
            "1": {
              "color": "#EF4444",
              "label": "Beginning",
              "sentiment": "needs work",
              "muiColor": "error"
            },
            "2": {
              "color": "#F59E0B",
              "label": "Developing",
              "sentiment": "improving",
              "muiColor": "warning"
            },
            "3": {
              "color": "#FB923C",
              "label": "Competent",
              "sentiment": "solid",
              "muiColor": "warning",
              "variant": "light"
            },
            "4": {
              "color": "#34D399",
              "label": "Proficient",
              "sentiment": "excellent",
              "muiColor": "success",
              "variant": "light"
            },
            "5": {
              "color": "#10B981",
              "label": "Advanced",
              "sentiment": "outstanding",
              "muiColor": "success"
            }
          }
        },
  
