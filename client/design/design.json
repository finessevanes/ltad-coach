    "specificDecisions": {
      "athleteAvatars": {
        "uploadSupport": false,
        "reasoning": "MVP simplicity - just initials, photos post-MVP. Reduces complexity and privacy concerns.",
        "initialsOnly": true,
        "shape": "circle",
        "size": "40px",
        "backgroundColors": [
          "#2563EB",
          "#F97316",
          "#10B981",
          "#8B5CF6",
          "#EC4899",
          "#06B6D4"
        ],
        "backgroundSelection": "Deterministic based on athlete name hash (same athlete = same color)",
        "fontSize": "16px",
        "fontWeight": 600,
        "textColor": "#FFFFFF",
        "muiComponent": "Avatar"
      },
  
      "emptyStates": {
        "style": "Clean icons + helpful text + clear CTA",
        "reasoning": "Illustrations can feel childish for coaches. Clean icons with clear actions are more professional.",
