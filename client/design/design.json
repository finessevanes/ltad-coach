            "action": "Add Athlete",
            "actionIcon": "AddCircleOutline"
          },
          "noTests": {
            "icon": "AssessmentOutlined",
            "iconLibrary": "@mui/icons-material",
            "heading": "No assessments recorded",
            "body": "Start your first assessment to track athlete progress and generate reports.",
            "action": "New Assessment",
            "actionIcon": "PlayCircleOutline"
          },
          "noReports": {
            "icon": "DescriptionOutlined",
            "iconLibrary": "@mui/icons-material",
            "heading": "No reports generated",
            "body": "Complete assessments to automatically generate progress reports.",
            "action": "View Assessments",
            "actionIcon": "ArrowForward"
          },
          "noResults": {
            "icon": "SearchOff",
            "iconLibrary": "@mui/icons-material",
            "heading": "No results found",
            "body": "Try adjusting your filters or search terms.",
            "action": "Clear Filters",
            "actionIcon": "FilterListOff"
          }
        },
        "muiComponents": ["Box", "Typography", "Button", "Icon"]
      },
  
      "errorHandling": {
        "strategy": "Severity-based display strategy",
        "reasoning": "Different errors need different levels of interruption. Critical errors block workflow, minor errors don't.",
        "critical": {
          "display": "Modal dialog (blocks interaction)",
          "examples": [
            "Camera access denied",
            "Video upload failed",
            "Server connection lost",
            "Authentication expired"
          ],
          "muiComponent": "Dialog",
          "actions": ["Retry", "Cancel"],
          "persistent": true
        },
        "important": {
          "display": "Alert banner at top of content area",
          "examples": [
            "Consent expired",
            "Athlete limit reached",
            "Storage quota exceeded",
            "Feature unavailable"
          ],
          "muiComponent": "Alert",
          "dismissible": true,
          "position": "top"
        },
        "minor": {
          "display": "Snackbar notification (bottom-center)",
          "examples": [
            "Settings saved",
            "Report sent",
            "Video processing started",
            "Data synced"
          ],
          "muiComponent": "Snackbar",
          "autoHideDuration": 4000,
          "position": "bottom-center"
        },
        "inline": {
          "display": "Helper text below field",
          "examples": [
            "Form validation errors",
            "Input format errors",
            "Field requirements"
          ],
          "muiComponent": "FormHelperText",
          "color": "error"
        }
